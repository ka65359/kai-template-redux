{
  "presets": [["env", { "modules": false }], "react", "stage-0"],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy"
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-react-remove-prop-types",
        "transform-react-constant-elements"
      ]
    },
    "test": {
      "presets": ["env", "react", "stage-0"]
    }
  }
}
